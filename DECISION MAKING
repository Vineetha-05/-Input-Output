x=a*( 350.34)+b*( 230.90)+c*(190.55)+d*(125.30)+e*(180.90)

if x<=15000:
    print("yes")
else:
    print("no")






Sece Dining

a=input()
b= int(input())

if a=="front" and b==1:
    print("left handed")
elif a=="front" and b==2:
    print("right handed")
elif a=="rear" and b==1:
    print("right handed")
elif a=="rear" and b==2:
    print("left handed")
else:
    print("invalid input")
    
    
    
    
    Intersection Of Circles
    
    x1=int(input())
y1=int(input())
r1=int(input())
x2=int(input())
y2=int(input())
r2=int(input())
r=r1+r2
m=((x1+x2)/2,(y1+y2)/2)

d=pow((x2-x1)*(x2-x1)+(y2-y1)*(y2-y1),0.5)

if d==r:
    print("the circles are tangential")
elif d<=r:
    print("the circles overlap")
else:
    print("the circles do not overlap")
    
    
    
    
    
    Microwave Oven
    
    a=int(input("enter the number of items\n"))
b=float(input("enter the single item heating time\n"))
if a==1:
    print("the recommended heating time is","%.2f"%b)
elif a<=3:
    
    c=float(b*50/100)
    d=float(b+c)
    print("the recommended heating time is","%.2f"%d)
else:
    print("number of items is more")
    
    
    
    
    
    
