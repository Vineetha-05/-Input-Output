Series 6

n=int(input())
i=1
pr=0
while i<=n:
    if(i%2==0):
        pr=pow(i,2)-2
        print(pr,end=' ')
    else:
        pr=pow(i,2)-1
        print(pr,end=' ')
    i+=1
    
    
    
    
    
    
    
    Series 7
    
    n=int(input())
a=1
b=2
for i in range(1,n+1):
    print(a,end=' ')
    print(b,end=' ')
    
    a=a*3
    b=b*3
    
    
    
    
    series 8
    
